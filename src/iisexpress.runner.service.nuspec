<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.12">
    <id>iisexpress.runner.service</id>
    <version>99.99.99-dev</version>
    <title>IISExpress Service Runner</title>
    <authors>Nathan Johnstone</authors>
    <summary>Service runner for IIS Express</summary>
    <description>Allows a website to be run as a service via IIS Express</description>
    <language>en-US</language>
    <projectUrl>https://github.com/nrjohnstone/IISExpress.Runners</projectUrl>
    <!--<iconUrl></iconUrl>-->
    <!--<licenseUrl></licenseUrl>-->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>false</developmentDependency>
    <dependencies>
      <dependency id="AmbientContext" version="0.0.1.7"  />
      <dependency id="AmbientContext.LogService.Serilog" version="0.2.0" />
      <dependency id="Serilog" version="2.3.0"  />
      <dependency id="Serilog.Settings.AppSettings" version="2.0.0"  />
      <dependency id="Serilog.Sinks.ColoredConsole" version="2.0.0" />
      <dependency id="Serilog.Sinks.File" version="3.1.0"  />
      <dependency id="Serilog.Sinks.RollingFile" version="3.2.0"  />
      <dependency id="Topshelf" version="4.0.3"  />
    </dependencies>
  </metadata>
  <files>
    <file src="service\bin\Release\IISExpressService.exe" target="tools\" />
    <file src="service\bin\Release\IISExpressService.exe.config" target="tools\" />    
  </files>
 
</package>